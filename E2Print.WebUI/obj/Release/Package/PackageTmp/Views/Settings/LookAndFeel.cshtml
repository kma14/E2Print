
@{
    ViewBag.Title = "LookAndFeel";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section PageStyles {
    <!-- Bootstrap styles -->
    @*<link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">*@
     Generic page styles 
    <link href="~/Scripts/jQuery-File-Upload-9.9.3/css/style.css" rel="stylesheet" />
    <!-- CSS to style the file input field as button and adjust the Bootstrap progress bars -->
    @*<link href="~/Scripts/jQuery-File-Upload-9.9.3/css/jquery.fileupload.css" rel="stylesheet" />*@
}
@section PageScripts{
    <script src="~/Scripts/jquery-ui-1.11.4.min.js"></script>
    <script src="~/Scripts/jQuery-File-Upload-9.9.3/jquery.fileupload.js"></script>
    <script src="~/Scripts/jQuery-File-Upload-9.9.3/jquery.iframe-transport.js"></script>
    <script src="~/Scripts/jQuery-File-Upload-9.9.3/jqueyFileUpload.js"></script>
}
<div class="generalContent">
    <div class="breadcrumb">
        <a href="/Admin/DashBoard" class="breadcrumb-other">Control Panel </a>
        <span class="entypo-right-open breadcrumb-delimiter"></span>
        <span class=".breadcrumb-current">Look and feel</span>
    </div>
    <div class="settings">
        <div class="fileUploadContainer">
            <span class="btn btn-success fileinput-button">
                <i class="glyphicon glyphicon-plus"></i>
                <span><b>Home page background image:</b></span>
                <!-- The file input field used as target for the file upload widget -->
                <input class="fileupload" type="file" name="files[]" data-url="/Photo/UploadPhoto/" multiple>
            </span>
            <br>
            <br>
            <!-- The global progress bar -->
            <div id="progress">
                <div class="progress-bar"></div>
            </div>
            <!-- The container for the uploaded files -->
            <div id="files" class="files"></div>
        </div>
    </div>
</div>


